<!DOCTYPE html>
<html>
  <head>
    <title>Cloudscape</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'application', "https://fonts.googleapis.com/css?family=Alegreya+Sans+SC:900&display=swap" %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%# <link href="https://fonts.googleapis.com/css?family=Alegreya+Sans+SC:900&display=swap" rel="stylesheet">  %>
  </head>

  <body>
    <div class="container">
      <%= render "partials/header" %>
      <%= yield %>
      <%= render "partials/footer" %>
    </div>
  </body>
</html>
